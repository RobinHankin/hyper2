citHeader("To cite in publications use:")

bibentry(
  bibtype  = "Article",
  title    = "{Partial rank data with the hyper2 package: likelihood functions for generalized {B}radley-{T}erry models}",
  author   = person("Robin K. S. Hankin"),
  journal  = "The R Journal",
  year     = "2017",
  volume   = "9",
  number   = "2",
  textVersion =
  paste("Hankin, R. K. S. (2017).",
  "Partial rank data with the hyper2 package: likelihood functions for generalized Bradley-Terry models",
     "The R Journal, volume 9, number 2")
)
